<% @spaces.each do |space| %>
  <div class="booyah-box col-xs-10 col-xs-offset-1">
    <h1><%= space.name %></h1>
    <i><%= space.address %></i><br /><br />
    <p><%= space.description %></p><br />
  </div>

  <hr />
<% end %>

<br class="clr">
<div class="text-center">
<%= paginate @spaces %>
</div>